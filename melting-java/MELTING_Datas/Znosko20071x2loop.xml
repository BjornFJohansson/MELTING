<!--/******************************************************************************/
    	/* contains the parameters for RNA/RNA 1x2 internal loop of ;
	REF: Brent M Znosko et al (2007). Biochemistry 46: 14715-14724. 
    	/******************************************************************************/-->
   			
   			 <mismatch type="initiation">
 				<enthalpy>3200.0<enthalpy>
 				<entropy>3.1</entropy>
 			</mismatch>
 			
 			<penalty type="per_AU_GU_closure">
 				<enthalpy>-3500.0<enthalpy>
 				<entropy>-13.6</entropy>
 			</penalty>
 			
 			<mismatch sequence="A/G_not_RA/YG" loop="1x2" type="first_non_canonical_pair">
 				<enthalpy>-6300.0</enthalpy>
 				<entropy>-16.2</entropy>
 			</mismatch>
 			<mismatch sequence="G/G" loop="1x2" type="first_non_canonical_pair">
 				<enthalpy>-6300.0</enthalpy>
 				<entropy>-16.2</entropy>
 			</mismatch>
 			<mismatch sequence="U/U" loop="1x2" type="first_non_canonical_pair">
 				<enthalpy>-11900.0</enthalpy>
 				<entropy>-35.5</entropy>
 			</mismatch>